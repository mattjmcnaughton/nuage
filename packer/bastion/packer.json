{
  "builders": [{
    "type": "vagrant",
    "communicator": "ssh",
    "source_path": "../base/output-vagrant/package.box",
    "provider": "virtualbox",
    "box_name": "ubuntu/bionic64",
    "teardown_method": "destroy",
    "add_force": true,
    "skip_package": true
  }],
  "provisioners": [{
    "type": "shell",
    "script": "../share/scripts/install-ansible.sh"
  }, {
    "type": "ansible",
    "playbook_file": "./playbook.yml"
  }]
}
